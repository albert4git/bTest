let SessionLoad = 1
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
cd ~/git/aTest/dotFiles/nVim/pdbA
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +2 NONE
badd +26 thrOne.py
badd +21 thrTwo.py
badd +39 thrLogThree.py
badd +38 thrDemonFour.py
badd +30 thrJoinFive.py
badd +48 thrJoinSix.py
badd +12 thrSeven.py
badd +16 thr8.py
badd +13 thr9Sign.py
badd +29 thr10Lock.py
badd +68 thr11LockHold.py
badd +16 thr12RLock.py
badd +21 thr13sync.py
badd +57 thr14Limit.py
badd +10 thr15Local.py
badd +11 thr16init.py
badd +0 thr17InitID.py
argglobal
silent! argdel *
$argadd NONE
$argadd thrOne.py
edit thr17InitID.py
set splitbelow splitright
wincmd t
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
argglobal
if bufexists('thr17InitID.py') | buffer thr17InitID.py | else | edit thr17InitID.py | endif
setlocal fdm=marker
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=6
setlocal fdn=20
setlocal nofen
let s:l = 18 - ((12 * winheight(0) + 18) / 36)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
18
normal! 0
lcd ~/git/aTest/dotFiles/nVim/pdbA
tabnext 1
if exists('s:wipebuf') && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20 winminheight=0 winminwidth=3 shortmess=filnxtToOFI
let s:sx = expand("<sfile>:p:r")."x.vim"
if file_readable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &so = s:so_save | let &siso = s:siso_save
doautoall SessionLoadPost
